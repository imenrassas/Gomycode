    "def factorielle(n):\n",
    "    if n<0:\n",
    "        print(\"Error\")\n",
    "    elif n==0:\n",
    "        print(1)\n",
    "    else:\n",
    "        x=1\n",
    "        while(n>1):\n",
    "            x*=n\n",
    "            n-=1\n",
    "            print(x)\n",
    "n=8;\n",
    "print(factorielle(n))"
   ]
